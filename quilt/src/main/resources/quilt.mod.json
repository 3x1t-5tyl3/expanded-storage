{
  "schema_version": 1,
  "quilt_loader": {
    "group": "semele.quinn.expandedstorage",
    "id": "expandedstorage",
    "version": "${version}",
    "metadata": {
      "name": "Expanded Storage",
      "description": "A mod adding various storage types, including double iron chests.",
      "contributors": {
        "Quinn Semele": "Owner"
      },
      "contact": {
        "homepage": "https://modrinth.com/mod/expanded-storage",
        "issues": "https://github.com/quinn-semele/expanded-storage/issues",
        "sources": "https://github.com/quinn-semele/expanded-storage/tree/expandedstorage/1.20.1"
      },
      "icon": "icon.png"
    },
    "intermediate_mappings": "net.fabricmc:intermediary",
    "entrypoints": {
      "main": "compasses.expandedstorage.impl.ThreadMain",
      "client": "compasses.expandedstorage.impl.ThreadClient",
      "emi": "compasses.expandedstorage.impl.client.compat.EmiCompat",
      "jei_mod_plugin": "compasses.expandedstorage.impl.client.compat.JeiCompat",
      "modmenu": "compasses.expandedstorage.impl.client.compat.ModMenuCompat",
      "rei_client": "compasses.expandedstorage.impl.client.compat.ReiCompat"
    },
    "depends": [
      {
        "id": "quilt_loader",
        "versions": ">=0.25.0"
      },
      {
        "id": "quilted_fabric_api",
        "versions": ">=10.0.0-alpha.1"
      },
      {
        "id": "minecraft",
        "versions": {
          "all": [">=1.20.5", "<1.21-alpha"]
        }
      }
    ]
  },
  "mixin": [
    "expandedstorage-common.mixins.json",
    "expandedstorage-thread.mixins.json"
  ]
}
